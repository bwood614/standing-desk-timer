{
  "icons": {
    "16": "./gen-assets/icon16.plasmo.png",
    "32": "./gen-assets/icon32.plasmo.png",
    "48": "./gen-assets/icon48.plasmo.png",
    "64": "./gen-assets/icon64.plasmo.png",
    "128": "./gen-assets/icon128.plasmo.png"
  },
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "./gen-assets/icon16.plasmo.png",
      "32": "./gen-assets/icon32.plasmo.png",
      "48": "./gen-assets/icon48.plasmo.png",
      "64": "./gen-assets/icon64.plasmo.png",
      "128": "./gen-assets/icon128.plasmo.png"
    },
    "default_popup": "./popup.html"
  },
  "version": "0.0.1",
  "author": "Conner&Brayden",
  "name": "DEV | Standing Desk Timer",
  "description": "An extension to motivate you to stand up.",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "static\\contents\\WidgetBar.tsx"
      ]
    }
  ],
  "host_permissions": [
    "https://*/*"
  ]
}
